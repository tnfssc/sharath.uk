    "langchain": "^0.1.37",
    "valibot": "^0.31.0-rc.6",
    "web-auth-library": "^1.0.3",
    "youtube-transcript": "^1.2.1",
    "youtubei.js": "^9.4.0"
  },
  "devDependencies": {
    "@cloudflare/workers-types": "^4.20240405.0",
    "@trivago/prettier-plugin-sort-imports": "^4.3.0",
